{
    "multipart": [
        {   "when": { "up": "true" },
            "apply": [
						{ "model": "block/brick_wall_post" },
						{ "model": "block/brick_wall_post_1" },
						{ "model": "block/brick_wall_post_2" },
						{ "model": "block/brick_wall_post_3" }
					]
        },
        {   "when": { "north": "low" },
            "apply": [
						{ "model": "block/brick_wall_side", "uvlock": true },
						{ "model": "block/brick_wall_side_1", "uvlock": true },
						{ "model": "block/brick_wall_side_2", "uvlock": true },
						{ "model": "block/brick_wall_side_3", "uvlock": true }
					]
        },
        {   "when": { "east": "low" },
            "apply": [
						{ "model": "block/brick_wall_side", "y": 90, "uvlock": true },
						{ "model": "block/brick_wall_side_1", "y": 90, "uvlock": true },
						{ "model": "block/brick_wall_side_2", "y": 90, "uvlock": true },
						{ "model": "block/brick_wall_side_3", "y": 90, "uvlock": true }
					]
        },
        {   "when": { "south": "low" },
            "apply": [
						{ "model": "block/brick_wall_side", "y": 180, "uvlock": true },
						{ "model": "block/brick_wall_side_1", "y": 180, "uvlock": true },
						{ "model": "block/brick_wall_side_2", "y": 180, "uvlock": true },
						{ "model": "block/brick_wall_side_3", "y": 180, "uvlock": true }
					]
        },
        {   "when": { "west": "low" },
            "apply": [
						{ "model": "block/brick_wall_side", "y": 270, "uvlock": true },
						{ "model": "block/brick_wall_side_1", "y": 270, "uvlock": true },
						{ "model": "block/brick_wall_side_2", "y": 270, "uvlock": true },
						{ "model": "block/brick_wall_side_3", "y": 270, "uvlock": true }
					]
        },
        {   "when": { "north": "tall" },
            "apply": [
						{ "model": "block/brick_wall_side_tall", "uvlock": true },
						{ "model": "block/brick_wall_side_tall_1", "uvlock": true },
						{ "model": "block/brick_wall_side_tall_2", "uvlock": true },
						{ "model": "block/brick_wall_side_tall_3", "uvlock": true }
					]
        },
        {   "when": { "east": "tall" },
            "apply": [
						{ "model": "block/brick_wall_side_tall", "y": 90, "uvlock": true },
						{ "model": "block/brick_wall_side_tall_1", "y": 90, "uvlock": true },
						{ "model": "block/brick_wall_side_tall_2", "y": 90, "uvlock": true },
						{ "model": "block/brick_wall_side_tall_3", "y": 90, "uvlock": true }
					]
        },
        {   "when": { "south": "tall" },
            "apply": [
						{ "model": "block/brick_wall_side_tall", "y": 180, "uvlock": true },
						{ "model": "block/brick_wall_side_tall_1", "y": 180, "uvlock": true },
						{ "model": "block/brick_wall_side_tall_2", "y": 180, "uvlock": true },
						{ "model": "block/brick_wall_side_tall_3", "y": 180, "uvlock": true }
					]
        },
        {   "when": { "west": "tall" },
            "apply": [
						{ "model": "block/brick_wall_side_tall", "y": 270, "uvlock": true },
						{ "model": "block/brick_wall_side_tall_1", "y": 270, "uvlock": true },
						{ "model": "block/brick_wall_side_tall_2", "y": 270, "uvlock": true },
						{ "model": "block/brick_wall_side_tall_3", "y": 270, "uvlock": true }
					]
        }
    ]
}
